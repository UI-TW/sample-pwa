{
  "name": "sample-pwa",
  "version": "1.0.0",
  "description": "simple web app for iTunes Search implemented using Vanila JS",
  "keywords": [
    "Vanila JS",
    "iTunes",
    "search",
    "apple",
    "babel",
    "ES6",
    "pwa"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:PWA-TW/sample-pwa.git"
  },
  "author": "Thoughworks <https://www.thoughtworks.com/>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "dev": "webpack-dev-server --progress --color --config ./webpack/webpack.config.dev.js",
    "build": "webpack --config ./webpack/webpack.config.prod.js --progress --colors --define process.env.NODE_ENV='\"production\"'",
    "compile": "npm run clean && npm run build && npm run pwa:setup",
    "lint": "eslint src/** webpack/** test/**",
    "lint:fix": "npm run lint -- --fix",
    "serve:dist": "http-server ./dist -p 3040",
    "start": "npm-run-all --parallel watch serve:dist",
    "watch": "watch \"npm run compile\" src",
    "pwa:setup": "workbox generateSW workbox-config.js"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "handlebars": "^4.0.11",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^4.4.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "extracted-loader": "^1.0.4",
    "file-loader": "^1.1.4",
    "handlebars-template-loader": "^0.8.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^6.0.17",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "prettier": "^1.3.1",
    "raf": "^3.4.0",
    "rimraf": "^2.5.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.6.2",
    "watch": "^1.0.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.4.2",
    "workbox-cli": "^3.6.3"
  }
}
